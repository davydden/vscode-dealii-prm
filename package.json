{
    "name": "dealii-prm",
    "displayName": "dealii-prm",
    "description": "Syntax highlighting for deal.II parameter files.",
    "version": "0.0.1",
    "publisher": "davydden",
    "engines": {
        "vscode": "^1.21.0"
